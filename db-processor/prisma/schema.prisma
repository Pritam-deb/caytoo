generator client {
  provider = "prisma-client-js"
  // binaryTargets = ["native", "linux-arm64-openssl-3.0.x"] //for Docker compatibility
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Article {
  id         String  @id @default(uuid())
  category   String?
  title      String
  content    String
  url_link   String  @unique
  image_url  String?
  created_at String
  updated_at String
  date       String
  pitched    Boolean @default(false)
}
